{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true,
    "File": true,
    "location": true,
    "localStorage": true
  },
  "rules": {
    "global-require": 0,
    "no-restricted-syntax": 0,
    "import/extensions": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "comma-dangle": [
      2,
      "always-multiline"
    ]
  }
}